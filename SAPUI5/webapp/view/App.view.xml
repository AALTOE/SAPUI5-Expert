<mvc:View
    controllerName="logaligroup.SAPUI5.controller.App"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
>
    <Shell id="shel1">
        <App id="app1">
            <pages>
                <Page
                    id="page1"
                    title="{i18n>appTitle}"
                >
                    <headerContent>
                        <Button
                            id="btn3"
                            icon="sap-icon://action"
                            press=".onOpendialogHeader"
                            class="sapUiSmallMarginEnd"
                        />
                    </headerContent>
                    <content>
                        <mvc:XMLView
                            id="Panelview1"
                            viewName="logaligroup.SAPUI5.view.HelloPanel"
                        />
                        <mvc:XMLView
                            id="Panelview2"
                            viewName="logaligroup.SAPUI5.view.InvoicesList"
                        />
                    </content>
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>
