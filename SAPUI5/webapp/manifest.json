{
  "_version": "1.65.0",
  "sap.app": {
    "id": "logaligroup.SAPUI5",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.15.0",
      "toolsId": "ecb4c365-81e8-4393-af9d-6f6434e79894"
    },
    "dataSources": {
      "northwind-entities" : {
        "type": "OData",
        "uri": "/v2/Northwind/Northwind.svc/",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.128.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "logaligroup.SAPUI5.i18n.i18n"
        }
      },
      "invoices": {
          "type": "sap.ui.model.json.JSONModel",
          "uri": "Invoices.json"
        },
      "northwind":{
        "dataSource": "northwind-entities"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "logaligroup.SAPUI5.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteApp",
          "pattern": "",
          "target": [
            "TargetOverview"
          ]
        },
        {
          "name": "Details",
          "pattern": "details/{invoicePath}",
          "target": [
            "TargetDetails"
          ]
        }
      ],
      "targets": {
        "TargetOverview": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Overview",
          "viewName": "Overview"
        },
        "TargetDetails": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Details",
          "viewName": "Details"
        }
      }
    },
    "rootView": {
      "viewName": "logaligroup.SAPUI5.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}